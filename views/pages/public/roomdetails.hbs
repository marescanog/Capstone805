<div class="container">
        <div class="mb-5 mt-4">
        </div>
        <div class="gallery">
            {{#each photos}}
                <figure class="gallery__item gallery__item--{{this.index}}">
                    <a target="_blank" href="{{this.url}}">
                        <img src="{{this.url}}" alt="{{this.alt}}" class="gallery__img"></a>
                </figure>
            {{/each }}
        </div>
        <div class="row mt-5">
            <div class="col-md-9">
                <div class="card mb-3" style="">
                    <div class="container ps-5 pt-4">
                        <h1>{{roomType}}</h1>
                         {{#unless NoOffer}}
                            <p><strong>{{offerName}} Included</strong></p>
                        {{/unless}}
                        <div class="container text-center">
                            <div class="row">
                                <div class="col align-self-start">
                                    <div class="room">
                                        {{!-- <span class="material-symbols-outlined">
                                            bedroom_child
                                        </span> --}}
                                        <img src="/img/icons/amenities/{{amenity1.icon}}" alt="Bed Icon" class="mb-2">
                                        <p > {{bedCount}} {{bedType}} Bed</p>
                                    </div>
                                </div>
                                <div class="col align-self-center">
                                    <div class="room">
                                        {{!-- <span class="material-symbols-outlined">
                                            bathtub
                                        </span> --}}
                                        <img src="/img/icons/amenities/{{amenity2.icon}}" alt="Amenity Icon" class="mb-2">
                                        <p> {{amenity2.name}}</p>
                                    </div>
                                </div>
                                <div class="col align-self-end">
                                    <div class="room">
                                        {{!-- <span class="material-symbols-outlined">
                                            soup_kitchen
                                        </span> --}}
                                        <img src="/img/icons/amenities/{{amenity3.icon}}" alt="Amenity Icon" class="mb-2">
                                        <p> {{amenity3.name}}
                                        </p>
                                    </div>
                                </div>
                                <div class="col align-self-end">
                                    <div class="room">
                                        {{!-- <span class="material-symbols-outlined">
                                            kitchen
                                        </span> --}}
                                        <img src="/img/icons/amenities/{{amenity4.icon}}" alt="Amenity Icon" class="mb-2">
                                        <p> {{amenity4.name}}
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="mt-5">
                            <h2>Description</h2>
                            <p class="text-justify">{{paragraph1}}</p>
                            <p class="text-justify">{{paragraph2}}</p>
                        </div>
                        <div class="mt-5 amenities">
                            <h2 class="text-warning-emphasis">
                                Offered Amenities</h2>
                            <ul class="row">
                                {{#each roomFeatures}}
                                    <li class="col-md-6">
                                        <img src="/img/icons/amenities/{{this.icon}}" alt="Amenity Icon" >
                                        <div style="width:0.6em"></div>
                                        {{this.name}}
                                    </li>
                                {{/each}}
                            </ul>

                        </div>
                        <div class="mt-5 amenities">
                            <h2>Safety and Hygiene</h2>
                            <ul class="row">
                                {{#each safetyandHygeine}}
                                    <li class="col-md-6">
                                        <img src="/img/icons/amenities/{{this.icon}}" alt="Amenity Icon">
                                        <div style="width:0.6em"></div>
                                        {{this.name}}
                                    </li>
                                {{/each}}

                                {{!-- <li class="col-md-6"><span class="material-symbols-outlined">
                                        fire_extinguisher
                                    </span>Fire Protection</li>
                                <li class="col-md-6"><span class="material-symbols-outlined">
                                        detector_smoke
                                    </span>Smoke Detectors</li>
                                <li class="col-md-6"><span class="material-symbols-outlined">
                                        sanitizer
                                    </span>Disinfections and Sterilizations</li> --}}
                            </ul>
                        </div>


                    </div>

                </div>
            </div>

            <div class="col-md-3">
                <div class="card1">
                    {{#unless NoOffer}}
                        <h5 class="card-title pt-5"><b>$ {{price}} / night</b></h5>
                    {{/unless}}
                    {{#if NoOffer}}
                        <p> *Prices may vary upon avilability & offers</p>
                    {{/if}}
                    <hr>

                    {{#each offersSidePanel}}
                        <p class="card-text">{{this.name}}</p>
                    {{/each}}
                    {{!-- 
                    <p class="card-text">Sq metres: 30</p>
                    <p class="card-text">In Room Dining </p>
                    <p class="card-text">Balcony Access </p>
                    <p class="card-text">Wifi Connectivity </p>
                    <p class="card-text">Laundry Services </p>
                    <p class="card-text">Concierge Services </p> --}}
                    {{#if NoOffer}}
                        <a href="/roomOffers?checkin=today&checkout=tomorrow&guests=1&rooms=1">
                        <button>
                            <strong>
                                Select a check in & check out date
                            </strong>
                        </button></a>
                    {{/if}}
                    {{#unless NoOffer}}
                        <a href="/checkout?roomdetails={{roomdetails}}&offers={{offerdetails}}&checkin={{checkin}}6&checkout={{checkout}}7&guests=1&rooms=1"><button>Reserve Now</button></a>
                    {{/unless}}
                </div>
            </div>
        </div>



    </div>

    </div>

<div class="mb-5 mt-4">
</div>